<%- include('partials/header') %>
<div class=" ui main text container segment">

    <div class="ui huge header">New Meeting</div>

    <form class="ui form" action="/create/<%=new_data.id%>" method="POST">
        <div class="field">
            <label>Title</label>
            <input type="text" name="title" placeholder="title" required/>
        </div>
        <div class="field">
            <label>Time</label>
            <input type="datetime-local" name="timing" placeholder="Timing" required/>
        </div>
        <div class="field">
            <label>With</label>
            <input type="text" name="withw" placeholder="With" required />
        </div>

        <input class="ui primary big inverted button" type="submit" />
    </form>

</div>


<%- include('partials/footer') %>